; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Asestheroids"
run/main_scene="res://src/app/program/program.tscn"
config/features=PackedStringArray("4.3", "C#", "Mobile")
boot_splash/bg_color=Color(0.356863, 0.431373, 0.490196, 1)
boot_splash/show_image=false
config/icon="res://icon.png"

[display]

window/size/viewport_width=720
window/size/viewport_height=1280
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/handheld/orientation=1

[dotnet]

project/assembly_name="aestheroids"

[editor]

naming/node_name_num_separator=2
naming/node_name_casing=2
naming/default_signal_callback_name="on_{node_name}_{signal_name}"
naming/default_signal_callback_to_self_name="on_{signal_name}"
naming/script_name_casing=2

[gui]

theme/custom="res://themes/default_theme.tres"
theme/custom_font="res://assets/fonts/molot.otf"
theme/default_font_hinting=2

[input_devices]

pointing/emulate_touch_from_mouse=true
pointing/android/enable_pan_and_scale_gestures=true

[internationalization]

locale/translations=PackedStringArray("res://assets/localization/test_localization.en.translation", "res://assets/localization/test_localization.es.translation", "res://assets/localization/test_localization.de.translation", "res://assets/localization/test_localization.fr.translation", "res://assets/localization/test_localization.it.translation", "res://assets/localization/test_localization.pt.translation", "res://assets/localization/test_localization.ja.translation", "res://assets/localization/test_localization.ko.translation", "res://assets/localization/test_localization.ru.translation", "res://assets/localization/test_localization.zh.translation", "res://assets/localization/test_localization.ar.translation", "res://assets/localization/test_localization.ca.translation", "res://assets/localization/test_localization.cs.translation", "res://assets/localization/test_localization.da.translation", "res://assets/localization/test_localization.fi.translation", "res://assets/localization/test_localization.hi.translation", "res://assets/localization/test_localization.hu.translation", "res://assets/localization/test_localization.nb.translation", "res://assets/localization/test_localization.nl.translation", "res://assets/localization/test_localization.pl.translation", "res://assets/localization/test_localization.sv.translation", "res://assets/localization/test_localization.th.translation", "res://assets/localization/test_localization.tr.translation", "res://assets/localization/test_localization.vi.translation")

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
